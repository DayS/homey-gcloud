{
    "actions": [
        {
            "id": "gcloud_logs_pushLog",
            "highlight": true,
            "title": {
                "en": "Push a log"
            },
            "titleFormatted": {
                "en": "Push a [[severity]] log to [[logger]]: [[text]]"
            },
            "args": [
                {
                    "name": "text",
                    "type": "text",
                    "placeholder": {
                        "en": "Text"
                    },
                    "title": {
                        "en": "Text"
                    }
                },
                {
                    "name": "logger",
                    "type": "text",
                    "required": false,
                    "placeholder": {
                        "en": ""
                    },
                    "title": {
                        "en": "Logger name (empty for default setting)"
                    }
                },
                {
                    "name": "severity",
                    "type": "dropdown",
                    "value": "DEFAULT",
                    "title": {
                        "en": "Severity"
                    },
                    "values": [
                        {
                            "id": "DEFAULT",
                            "title": {
                                "en": "Default"
                            }
                        },
                        {
                            "id": "DEBUG",
                            "title": {
                                "en": "Debug"
                            }
                        },
                        {
                            "id": "INFO",
                            "title": {
                                "en": "Info"
                            }
                        },
                        {
                            "id": "NOTICE",
                            "title": {
                                "en": "Notice"
                            }
                        },
                        {
                            "id": "WARNING",
                            "title": {
                                "en": "Warning"
                            }
                        },
                        {
                            "id": "ERROR",
                            "title": {
                                "en": "Error"
                            }
                        },
                        {
                            "id": "CRITICAL",
                            "title": {
                                "en": "Critical"
                            }
                        },
                        {
                            "id": "ALERT",
                            "title": {
                                "en": "Alert"
                            }
                        },
                        {
                            "id": "EMERGENCY",
                            "title": {
                                "en": "Emergency"
                            }
                        }
                    ]
                }
            ]
        },
        {
            "id": "gcloud_storage_uploadImage",
            "highlight": true,
            "title": {
                "en": "Upload an image"
            },
            "titleFormatted": {
                "en": "Upload image [[droptoken]] in bucket [[bucketName]] at [[destinationPath]]"
            },
            "droptoken": [
                "image"
            ],
            "args": [
                {
                    "name": "bucketName",
                    "type": "text",
                    "title": {
                        "en": "Bucket name (empty for default setting)"
                    }
                },
                {
                    "name": "destinationPath",
                    "type": "text",
                    "title": {
                        "en": "Destination path"
                    }
                }
            ]
        }
    ]
}